#!/bin/bash
name=${0##/*/}.$1.$2 
rm datg.$name gggtitle1 gggargs gggeref
echo fsmom= $1
echo atomz= $2
echo 'comninedname= ',$name
for bzw in .01 .003 .001
do
for rstar in .6 .7 .8 .9 1
do
for pwemax in 4  
do
pldimer1 fsmom=$1 atomz=$2 pwemax=$pwemax order=dis bzw=$bzw rstar=$rstar >>datg.$name
echo >>datg.$name
echo >>datg.$name
done
done
done
plmol.py $name >gggmol.$name
chmod +x gggmol.$name
./gggmol.$name
