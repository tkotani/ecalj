#!/bin/bash
#@$-q ha-oc130033
#@$-N 1
#@$-J T24
#@$-lE 88:00:00
#@$-lm 30gb
MAT=cu
EXE=~/binb2f0

cd $QSUB_WORKDIR
h_mpdboot

echo '+gwsc started :' `date` $PBS_O_WORKDIR '+'
ulimit -s unlimited
${EXE}/genMLWF -np 24 $MAT >& out

echo '-gwsc finished :' `date` $PBS_O_WORKDIR '-'
mpdallexit


# -- Sergey.F the screened coulom interaction 
