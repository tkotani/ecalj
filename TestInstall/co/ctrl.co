TESTLMF lmfa  co -vmet=4 -vlmf=1 -vnk=8 -vnit=3 --pr31 --no-iactiv
        lmf   co -vmet=4 -vlmf=1 -vnk=8 -vnit=3 -vw2=0 --pr31 --no-iactiv --rs=0
        rm mixm.co
        lmf   co -vmet=4 -vlmf=1 -vnk=8 -vnit=3 -vw1=0 --pr31 --no-iactiv 
        rm mixm.co
        lmf   co -vmet=4 -vlmf=1 -vnk=8 -vnit=3 -vw1=1 --pr31 --no-iactiv --time=5
#       lmf   co -vmet=4 -vlmf=1 -vnk=8 -vnit=3 --pr31 --no-iactiv --time=5 --band:fn=syml
#       This one assigns weights to majority bands
#       lmf   co -vmet=4 -vlmf=1 -vnk=8 -vnit=3 --pr31 --no-iactiv -vso=t --band~col=1:18~fn=syml
#       This one assigns weights to Co d bands
#       lmf   co -vmet=4 -vlmf=1 -vnk=8 -vnit=3 --pr31 --no-iactiv -vso=t --band~col=5:9,14:18,18+5:18+9,18+14:18+18~fn=syml
#       This one assigns 1st weight to majority Co d band, 2nd weight to minority
        lmf   co -vmet=4 -vlmf=1 -vnk=8 -vnit=3 --pr31 --no-iactiv -vso=t --band~col=5:9,14:18~col2=18+5:18+9,18+14:18+18~fn=syml
MPIK version
        mpix -np=13 lmf-MPIK   co -vmet=4 -vlmf=1 -vnk=8 -vnit=3 -vw2=0 --pr31 --no-iactiv --rs=0
        rm mixm.co
        mpix -np=13 lmf-MPIK   co -vmet=4 -vlmf=1 -vnk=8 -vnit=3 -vw1=0 --pr31 --no-iactiv
        rm mixm.co
        mpix -np=13 lmf-MPIK   co -vmet=4 -vlmf=1 -vnk=8 -vnit=3 -vw1=1 --pr31 --no-iactiv --time=5
        mpix -np=13 lmf-MPIK   co -vmet=4 -vlmf=1 -vnk=8 -vnit=3 --pr31 --no-iactiv -vso=t --band~fn=syml
TSTPDOS lmfa  co -vmet=4 -vlmf=1 -vnk=8 -vnit=1  --pr31 --no-iactiv
        lmf   co -vmet=4 -vlmf=1 -vnk=8 -vnit=1  --pr31 --no-iactiv -vpdos=t --pdos:mode=2:sites=2
        mv dos.co tdos.co
        echo 501 -.8 .7 | lmdos co -vmet=4 -vlmf=1 -vnk=8 -vnit=1  --pr31 --no-iactiv -vpdos=t --pdos:mode=2:sites=2
        mv dos.co pdos.co
