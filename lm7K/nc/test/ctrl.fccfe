% const nit=1 wsr=2.65 qss=0 nk=8 nk2=nk sdyn=f sdmod=1 ccor=t twoc=f ldmat=0 nc=t idxp=1 grp2=1
% char0 strx 3k
% echo  fcc Fe in {strx} structure  qss={qss}
#Tests: For all '3k' test cases, do:
#       lmstr fccfe; rm {a*,eula,ma,mq}.fccfe; lm -vnit=0 fccfe --no-iactive
#       To check equilibrium 3k structure:
#       lm fccfe --no-iactive
#       To test mode=0,1,1001 (see output files this directory)
#       rm {a*,eula,ma,mq}.fccfe; echo q | lm -vnit=0 fccfe
#       cp nc/test/eula.ran eula.fccfe
#       echo i | lm -vnk=4 -vnit=4 -vsdmod=0 -vsdyn=t fccfe
#       ... or use ... -vsdmod=1, or eula-l.ran for l-dependent case
#       To test lm-dependent euler angles, invoke:
#       cp nc/test/eula-lm.ran eula.fccfe
#       echo i | lm -vnk=4 -vnit=4 -vsdmod=1 fccfe
        NB: same GRP2 artifically forces all moments same
TESTDNF lmstr fccfe
        lm -vnit=0 -vsdyn=t -vidxp=2 fccfe --no-iactive
        lm -vnit=1 -vsdyn=t -vidxp=2 fccfe --no-iactive
TEST    lmstr fccfe
        lm -vnit=0 -vsdyn=t fccfe --no-iactive
        lm -vnit=1 -vsdyn=t fccfe --no-iactive
HEADER  Test of fcc fe, 2k and 3k structures
CMD     -dup=2,2,2
% ifdef strx=='2k'
STRUC   NBAS=4 NSPEC=1 ALAT=a PLAT=1 0 0  0 1 0  0 0 1
SYMGRP  R4Z MX MY MZ*R4Z
CLASS   ATOM=A  Z=26 EREF=-2540.5681 A=.01 NR=901 R/W=1 GRP2={grp2}
SITE    ATOM=A  POS=   0   0   0   ROT=(1,-1,0)-pi/2
        ATOM=A  POS=  .5  0.5 .0   ROT=(1,-1,0)pi/2
        ATOM=A  POS=  .0  0.5 .5   ROT=(1,1,0)-pi/2
        ATOM=A  POS=  .5  0.0 .5   ROT=(1,1,0)pi/2
% elseifd strx=='3k'
# NB: qss=1 should generate same results as qss=0
STRUC   NBAS=4 NSPEC=1 ALAT=a PLAT=1 0 0  0 1 0  0 0 1
SYMGRP  MX MY {?~qss==0~I~} {?~(sdyn==0&ldmat==0)~R4Z~}
CLASS   ATOM=A  Z=26 EREF=-2540.5681 A=.01 NR=901 R/W=1 GRP2={grp2} IDXDN=1 {idxp} 1
# Note: pi/3.2885 = acos(1/sqrt(3)) and pi/1.43696 = acos(-1/sqrt(3))
SITE    ATOM=A  POS=   0   0   0   ROT=(1,-1,0)-pi/3.2885
        ATOM=A  POS=  .5  0.5 .0   ROT=(1,-1,0)pi/3.2885
        ATOM=A  POS=  .0  0.5 .5   ROT=(1,1,0)-pi/1.43696
        ATOM=A  POS=  .5  0.0 .5   ROT=(1,1,0)pi/1.43696
% elseifd strx=='fm'
STRUC   NBAS=4 NSPEC=1 ALAT=a PLAT=1 0 0  0 1 0  0 0 1
SYMGRP  R4Z MX MY MZ*R4Z
CLASS   ATOM=A  Z=26 EREF=-2540.5681 A=.01 NR=901 R/W=1 GRP2={grp2}
SITE    ATOM=A  POS=   0   0   0   ROT=(1,-1,0)-pi/2*0
        ATOM=A  POS=  .5  0.5 .0   ROT=(1,-1,0)pi/2*0
        ATOM=A  POS=  .0  0.5 .5   ROT=(1,1,0)-pi/2*0
        ATOM=A  POS=  .5  0.0 .5   ROT=(1,1,0)pi/2*0
% elseifd strx=='ss'
