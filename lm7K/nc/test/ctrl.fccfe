        AMODE=A0,w=0,0,wa=1,fn=ma,n=2,b=1
% const beta=0
SITE    ATOM=A1 POS=  0  0   0   ROT=z:pi,y:{beta}
        ATOM=A2 POS= .0 .5  .5   ROT=z:pi,y:{pi-3*beta}
        ATOM=A3 POS=  0  0   1   ROT=z:pi,y:{pi-2*beta}
        ATOM=A4 POS=  0 .5 1.5   ROT=z:pi,y:0
