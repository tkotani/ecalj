#       ALAT=a PLAT=1/2 sqrt(3)/2 0 1/2 -sqrt(3)/2 0   0 0 cbya
        ALAT=a PLAT=0 -1 0  sqrt(3)/2 1/2 0   0 0 cbya
        ALAT=a PLAT=1 0 0   1/2 sqrt(3)/2 0   0 0 cbya
%ifdef pdos
SYMGRP  i*i
%endif
SYMGRP  find
SYMGRP  R6Z:T(.5,1/sqrt(12),cbya/2) MZ MX
%elseifd bct
        ALAT=a PLAT= -.5  .5  {.5*cbct/abct}
                      .5 -.5  {.5*cbct/abct}
                      .5  .5 -{.5*cbct/abct}
SYMGRP R4Z MZ R2X
%elseifd fcc
        ALAT=a PLAT=  .0 .5 .5  .5  .0 .5  .5 .5  .0
%elseifd bcc
        ALAT=a PLAT= -.5 .5 .5  .5 -.5 .5  .5 .5 -.5
% endif
% ifdef stet
