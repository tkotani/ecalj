        ATOM=A3 Z=26 EREF=-2540.5681 A=.01 NR=901 R/W=1 GRP2={grp2}
        ATOM=A4 Z=26 EREF=-2540.5681 A=.01 NR=901 R/W=1 GRP2={grp2}
