SYMGRP
CLASS
% repeat i1 = 0,1
% repeat i2 = 0,1
% repeat i3 = 0,1
        ATOM=a{i1}{i2}{i3}  Z=26 EREF=-2540.5681 A=.01 NR=901 R/W=1
        ATOM=b{i1}{i2}{i3}  Z=26 EREF=-2540.5681 A=.01 NR=901 R/W=1
